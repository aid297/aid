{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "singleQuote": false,
    "printWidth": 999,
    "tabWidth": 4,
    "useTabs": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "embeddedLanguageFormatting": "auto"
}