{
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": true,
    "singleQuote": false,
    "printWidth": 9999,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "embeddedLanguageFormatting": "auto",
    "vueIndentScriptAndStyle": false
}
