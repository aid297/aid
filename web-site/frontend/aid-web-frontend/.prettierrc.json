{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": true,
	"singleQuote": true,
	"printWidth": 9999,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"embeddedLanguageFormatting": "auto",
	"vueIndentScriptAndStyle": false
}
